<Window x:Class="prototype_Info_Manage.CredentialCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prototype_Info_Manage"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterOwner"
        Title="CredentialCheck" Height="220" Width="320">
    <Grid>

        <TextBox x:Name="ID_CredentialCheck" HorizontalAlignment="Center" Margin="0,89,0,0" Text="TextBox" VerticalAlignment="Top" Width="270" Height="42" FontSize="18" TextAlignment="Center"/>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Please Scan Your ID." VerticalAlignment="Top" Height="30" Width="200" FontSize="16" FontWeight="Bold" TextAlignment="Center" Margin="0,46,0,0"/>
    </Grid>
</Window>
